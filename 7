const dayOfWeek = ["Domingo","Lunes", "Martes", "Miercoles", "Jueves", "Viernes", "Sabado"]

var currentdate = new Date(); 

const month = currentdate.toLocaleString('es-mx', { month: 'long' });
console.log(month);

dateString = `${dayOfWeek[currentdate.getDay()]} de ${currentdate.getMonth()}`

document.getElementById('fecha').innerHTML = dateString
